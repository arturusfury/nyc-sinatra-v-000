      <div class="starter-template">
        <h1>List of Landmarks</h1>
        <% @landmarks.each do |landmark| %>
        <p class="lead"><a href="/landmarks/<%=landmark.id%>"><%= landmark.name %> (<%= landmark.year_completed %>)</a></p>
        <% end %>
        <div class="form-group row">
          <div class="col-xs-offset-4 col-xs-4">
            <a href="/landmarks/new" class="btn btn-primary btn-block btn-sm" role="button">Create</a>
          </div>
        </div>
      </div>
