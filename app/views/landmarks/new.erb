<div class="starter-template">
  <form method="post" action="/landmarks">
    <div class="form-group row">
      <h3>Enter an Landmark name</h3>
    </div>
    <div class="form-group row">
      <div class="col-xs-offset-4 col-xs-4">
        <input class="form-control" type="text" name="landmark[name]" placeholder="ex: St. Louis Arch" id="landmark_name">
      </div>
    </div>
    <div class="form-group row">
      <h3>Enter the year completed</h3>
    </div>
    <div class="form-group row">
      <div class="col-xs-offset-4 col-xs-4">
        <input class="form-control" type="number" name="landmark[year_completed]" placeholder="ex: 1965" id="landmark_year_completed">
      </div>
    </div>
    <div class="form-group row">
      <div class="col-xs-offset-4 col-xs-4">
        <button type="submit" class="btn btn-primary btn-block">Create New Landmark</button>
      </div>
    </div>
  </form>
</div>
